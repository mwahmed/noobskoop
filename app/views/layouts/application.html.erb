<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <title>EngNuke | <%= @title %></title>
  <meta http-equiv="Content-Language" content="English" />
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <link href="tabs.css" rel="stylesheet" type="text/css" /is this>
  <link href="menu.css" rel="stylesheet" type="text/css" media="screen" />
<%= javascript_include_tag "jsShare.js" %>
<%= javascript_include_tag "jquery-1.4.4.min.js" %>
<%= javascript_include_tag 'jquery-1.4.2.min', 'rails', 'application' %>

  <%= javascript_include_tag "easytabs.js" %>
  <%= stylesheet_link_tag "fullcalendar.css" %>
  <%= stylesheet_link_tag "application.css" %>
  <%= javascript_include_tag "jquery.js" %>
  <%= javascript_include_tag "jquery.rest.js" %>
  <%= javascript_include_tag "rails.js" %>
  <%= javascript_include_tag "application.js" %>
   <%= render 'layouts/stylesheets' %>
    <%= javascript_include_tag :defaults %>
  <!-- these are needed for the calendar. -->
  <%= javascript_include_tag "jquery-ui-1.8.11.custom.min.js" %>
  <%= javascript_include_tag "fullcalendar.js" %>
  <%= javascript_include_tag "calendar.js" %>
  <%= csrf_meta_tag %>  
  <%= javascript_tag "var AUTH_TOKEN = #{form_authenticity_token.inspect};" if protect_against_forgery? %>

 <script type="text/javascript">
        $(document).ready(function() {
            $('#buttons').jsShare({ maxwidth: 360 });
            $('#buttons-expanded').jsShare({ initialdisplay: 'expanded', maxwidth: 360 });
        });
    </script>

    <style type="text/css">
       
        #demo
        {
            width: 700px;
            margin: auto;
            height: 78px;
            text-align: center;
            padding-top: 140px;
        }
        #demo p
        {
            text-align: left;
            color: #b5B5B5;
        }
    </style>

</head>
<body>
    <div class="container">

      <%= render 'layouts/header' %>
</br>
<section class="round">
<div class="menu1">
<ul>
<li><a href="#" onmouseover="easytabs('1', '1');" onfocus="easytabs('1', '1');" onclick="return false;"  title="" id="tablink1">Go To Course</a></li>
<li><a href="#" onmouseover="easytabs('1', '2');" onfocus="easytabs('1', '2');" onclick="return false;"  title="" id="tablink2">Search Tags</a></li>
<li><a href="#" onmouseover="easytabs('1', '3');" onfocus="easytabs('1', '3');" onclick="return false;"  title="" id="tablink3">Search Questions</a></li>
<li><a href="#" onmouseover="easytabs('1', '4');" onfocus="easytabs('1', '4');" onclick="return false;"  title="" id="tablink4">Search Users</a>
</li>
</ul>
</div>
<!--End of the Tabmenu1 -->


<!--Start Tabcontent 1 -->
<div id="tabcontent1"><h6><% form_tag check_course_path, :method => 'get' do %>
<p>
    <%= text_field_tag :course, params[:course] %>
    <%= submit_tag "Go To Course", :name => nil %>
<br />
    example: ECE361
<%end%>
</p></h6></div>
<!--End Tabcontent 1-->

<!--Start Tabcontent 2-->
<div id="tabcontent2"><% form_tag check_tag_path, :method => 'get' do %>
<p>
    <%= text_field_tag :course, params[:course] %>
    <%= submit_tag "Search Tags", :name => nil %>
<br />
</p>
  <% end %></div>
<!--End Tabcontent 2 -->

<!--Start Tabcontent 3-->
<div id="tabcontent3"><% form_tag qsearch_path, :method => 'get' do %>
<p>
    <%= text_field_tag :query, params[:query] %>
    <%= submit_tag "Search User", :name => nil %>
</p>
  <% end %></div>
<!--End Tabcontent 3-->

<!--Start Tabcontent 4-->
<div id="tabcontent4"><% form_tag search_path, :method => 'get' do %>
<p>
    <%= text_field_tag :query, params[:query] %>
    <%= submit_tag "Search User", :name => nil %>
</p>
  <% end %></div>
<!--End Tabcontent 4-->



<p>____________________________________________________________________________________________________________________________________________________________________</p>      
	<% flash.each do |key, value| %>
         <%= content_tag(:div, value, :class => "flash #{key}") %>
        <% end %>

<div id="wrap">
<div id="right">

<div class="articles">
</br>
<%= content_tag :p, notice, :class => 'notice' if notice.present? %>
<%= content_tag :p, alert, :class => 'alert' if alert.present? %>

        <%= yield %>

</br>
<br /><br />
</div>
</div>

<div id="left"> 

<h3>Disciplines</h3>
<ul>
<!--h6><a href=/ece>ECE</a></h6-->
<h6><%= link_to 'ECE', '/ECE' %></h6>
<h6><a href="/d_threads">Latest Question</a></h6>
</ul>

</div>

<div style="clear: both;"> </div>
</div>

      </section>
      <%= render 'layouts/footer' %>
    </div>
</br>
  </body>
</html>

